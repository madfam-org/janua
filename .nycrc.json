{
  "all": true,
  "check-coverage": true,
  "reporter": ["text", "lcov", "html", "json"],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.test.tsx",
    "**/*.spec.tsx",
    "**/test_*.py",
    "**/__tests__/**",
    "**/tests/**",
    "**/dist/**",
    "**/build/**",
    "**/.next/**",
    "**/node_modules/**",
    "**/coverage/**"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
