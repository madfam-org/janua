{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://plinto-api.railway.app/api/:path*"
    },
    {
      "source": "/.well-known/jwks.json",
      "destination": "https://plinto-api.railway.app/.well-known/jwks.json"
    },
    {
      "source": "/.well-known/openid-configuration",
      "destination": "https://plinto-api.railway.app/.well-known/openid-configuration"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/.well-known/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=600"
        }
      ]
    }
  ]
}