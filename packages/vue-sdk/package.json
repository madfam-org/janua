{
  "name": "@janua/vue-sdk",
  "version": "0.1.0",
  "description": "Official Janua SDK for Vue 3 applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "janua",
    "vue",
    "vue3",
    "authentication",
    "auth",
    "composables",
    "plugin"
  ],
  "author": "Janua Team",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/madfam-org/janua.git",
    "directory": "packages/vue-sdk"
  },
  "homepage": "https://janua.dev",
  "bugs": {
    "url": "https://github.com/madfam-org/janua/issues"
  },
  "dependencies": {
    "@janua/typescript-sdk": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vitest": "^1.0.0",
    "vue": "^3.5.28"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.madfam.io/"
  }
}
