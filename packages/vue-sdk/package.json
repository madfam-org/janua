{
  "name": "@plinto/vue",
  "version": "1.0.0",
  "description": "Official Plinto SDK for Vue 3 applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "plinto",
    "vue",
    "vue3",
    "authentication",
    "auth",
    "composables",
    "plugin"
  ],
  "author": "Plinto Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/madfam-io/plinto.git",
    "directory": "packages/vue-sdk"
  },
  "homepage": "https://plinto.dev",
  "bugs": {
    "url": "https://github.com/madfam-io/plinto/issues"
  },
  "dependencies": {
    "@plinto/typescript-sdk": "^1.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}