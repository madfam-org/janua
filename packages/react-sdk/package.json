{
  "name": "@janua/react-sdk",
  "version": "0.1.0",
  "description": "Official Janua SDK for React applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:js-only": "tsup src/index.ts --format cjs,esm --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --watch"
  },
  "dependencies": {
    "@janua/typescript-sdk": "0.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "janua",
    "react",
    "authentication",
    "auth",
    "hooks",
    "components",
    "sso",
    "oauth",
    "mfa",
    "passkeys"
  ],
  "author": "Janua Team <team@janua.dev>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/madfam-io/janua.git",
    "directory": "packages/react-sdk"
  },
  "bugs": {
    "url": "https://github.com/madfam-io/janua/issues"
  },
  "homepage": "https://janua.dev",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.madfam.io/"
  }
}
