{
  "name": "@janua/nextjs",
  "version": "0.1.0",
  "description": "Official Janua SDK for Next.js applications with App Router and Pages Router support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app/index.d.ts",
      "import": "./dist/app/index.mjs",
      "require": "./dist/app/index.js"
    },
    "./pages": {
      "types": "./dist/pages/index.d.ts",
      "import": "./dist/pages/index.mjs",
      "require": "./dist/pages/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "janua",
    "nextjs",
    "authentication",
    "auth",
    "next",
    "app-router",
    "pages-router",
    "middleware"
  ],
  "author": "Janua Team",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/madfam-org/janua.git",
    "directory": "packages/nextjs-sdk"
  },
  "homepage": "https://janua.dev",
  "bugs": {
    "url": "https://github.com/madfam-org/janua/issues"
  },
  "dependencies": {
    "@janua/typescript-sdk": "workspace:*",
    "jose": "^5.0.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^18.0.0",
    "next": "15.1.6",
    "react": "18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.madfam.io/"
  }
}
