{
  "name": "@plinto/nextjs",
  "version": "0.1.0",
  "description": "Official Plinto SDK for Next.js applications with App Router and Pages Router support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./app": {
      "require": "./dist/app/index.js",
      "import": "./dist/app/index.mjs",
      "types": "./dist/app/index.d.ts"
    },
    "./pages": {
      "require": "./dist/pages/index.js",
      "import": "./dist/pages/index.mjs",
      "types": "./dist/pages/index.d.ts"
    },
    "./middleware": {
      "require": "./dist/middleware.js",
      "import": "./dist/middleware.mjs",
      "types": "./dist/middleware.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "plinto",
    "nextjs",
    "authentication",
    "auth",
    "next",
    "app-router",
    "pages-router",
    "middleware"
  ],
  "author": "Plinto Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/plinto/plinto",
    "directory": "packages/nextjs-sdk"
  },
  "homepage": "https://plinto.dev",
  "bugs": {
    "url": "https://github.com/plinto/plinto/issues"
  },
  "dependencies": {
    "@plinto/js": "^0.1.0",
    "jose": "^5.0.0"
  },
  "peerDependencies": {
    "next": ">=12.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}