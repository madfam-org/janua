# Janua Dashboard Environment Variables
# Copy this file to .env.local and fill in the values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8001
NEXT_PUBLIC_API_VERSION=v1

# Authentication
NEXT_PUBLIC_JANUA_ISSUER=https://janua.dev
NEXT_PUBLIC_JANUA_AUDIENCE=janua.dev
NEXT_PUBLIC_JANUA_TENANT_ID=tenant_123
NEXT_PUBLIC_JANUA_JWKS_URL=https://janua.dev/.well-known/jwks.json

# App Configuration
NEXT_PUBLIC_APP_URL=https://app.janua.dev
NEXT_PUBLIC_APP_NAME="Janua Dashboard"

# Feature Flags
NEXT_PUBLIC_ENABLE_WEBHOOKS=false
NEXT_PUBLIC_ENABLE_AUDIT_LOG=false
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Session Configuration
SESSION_SECRET=your-session-secret-here
SESSION_MAX_AGE=86400

# Monitoring (optional)
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_POSTHOG_KEY=