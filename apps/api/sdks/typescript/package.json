{
  "name": "@janua/typescript-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Janua authentication platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "generate": "openapi-generator-cli generate",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "authentication",
    "auth",
    "janua",
    "sdk",
    "typescript",
    "oauth",
    "mfa",
    "sso",
    "identity"
  ],
  "author": "Janua Team <team@janua.dev>",
  "license": "AGPL-3.0",
  "homepage": "https://janua.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/madfam-io/janua.git",
    "directory": "apps/api/sdks/typescript"
  },
  "bugs": {
    "url": "https://github.com/madfam-io/janua/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
